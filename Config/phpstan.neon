parameters:
	ignoreErrors:
		- '#Access to protected property Web\WebApplication::\$[a-zA-Z0-9_]+#'
		- '#Calling method prepare\(\) on possibly null value of type PDO\|null\.#'
		- '#Calling method lastInsertId\(\) on possibly null value of type PDO\|null\.#'
	fileExtensions:
		- php
	excludes_analyse:
		- *.tpl.php